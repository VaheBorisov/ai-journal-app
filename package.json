    "@tamagui/lucide-icons": "^1.135.4",
    "@tamagui/toast": "^1.135.4",
    "babel-preset-expo": "~13.0.0",
    "burnt": "^0.12.2",
    "expo": "53.0.23",
    "expo-auth-session": "^7.0.8",
    "expo-build-properties": "~0.14.8",
    "expo-font": "~13.3.2",
