    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.5",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
